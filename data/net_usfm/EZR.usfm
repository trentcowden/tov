\v 22  \w Exercise appropriate caution|strong="H2095"\w* \w so that there is|strong="H1934"\w* \w no negligence|strong="H7960"\w* \w in|strong="H5922"\w* \w this|strong="H1836"\w* \w matter. Why|strong="H4101"\w* \w should danger|strong="H2257"\w* \w increase|strong="H7680"\w* \w to the point that kings|strong="H4430"\w* \w sustain damage|strong="H5142"\w*?” 
\p
\v 23  \w Then|strong="H0116"\w*, \w as soon as|strong="H1768"\w* \w the copy|strong="H6573"\w* \w of the letter|strong="H5407"\w* \w from|strong="H1768"\w* \w King|strong="H4430"\w* \w Artaxerxes|strong="H0783"\w* \w was read|strong="H7123"\w* \w in the presence|strong="H6925"\w* \w of Rehum|strong="H7348"\w*, \w Shimshai|strong="H8124"\w* \w the scribe|strong="H5613"\w*, \w and their colleagues|strong="H3675"\w*, \w they proceeded|strong="H0236"\w* \w promptly|strong="H0924"\w* \w to|strong="H5922"\w* \w the Jews|strong="H3062"\w* \w in Jerusalem|strong="H3390"\w* \w and stopped|strong="H0989"\w* \w them|strong="H1994"\w* \w with threat|strong="H0153"\w* \w of armed force|strong="H2429"\w*. 
\p
\v 24  \w So|strong="H0116"\w* \w the work|strong="H5673"\w* \w on the temple|strong="H1005"\w* \w of God|strong="H0426"\w* \w in Jerusalem|strong="H3390"\w* \w came to a halt|strong="H0989"\w*. \w It remained|strong="H1934"\w* \w halted|strong="H0989"\w* \w until|strong="H5705"\w* \w the second|strong="H8648"\w* \w year|strong="H8140"\w* \w of the reign|strong="H4437"\w* \w of King|strong="H4430"\w* \w Darius|strong="H1868"\w* \w of Persia|strong="H6540"\w*. 
\c 5  
\p
\v 1  \w Then the prophets|strong="H5013"\w* \w Haggai|strong="H2292"\w* \w and Zechariah|strong="H2148"\w* \w son|strong="H1247"\w* \w of Iddo|strong="H5714"\w* \w prophesied|strong="H5013"\w* \w concerning|strong="H5922"\w* \w the Jews|strong="H3062"\w* \w who|strong="H1768"\w* \w were in Judah|strong="H3061"\w* \w and Jerusalem|strong="H3390"\w* \w in the name|strong="H8036"\w* \w of the God|strong="H0426"\w* \w of Israel|strong="H3479"\w* \w who was over|strong="H5922"\w* them. 
