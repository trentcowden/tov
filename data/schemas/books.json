{
  "$schema": "https://json-schema.org/draft-07/schema",
  "items": {
    "additionalProperties": false,
    "properties": {
      "bookId": {
        "$ref": "./ids.json#/properties/bookId"
      },
      "chapters": {
        "items": {
          "type": "string"
        }
      },
      "englishDivision": {
        "$ref": "./ids.json#/properties/englishDivision"
      },
      "hebrewDivision": {
        "$ref": "./ids.json#/properties/hebrewDivision"
      },
      "hebrewOrder": {
        "type": [
          "number",
          "null"
        ]
      },
      "name": {
        "type": "string"
      },
      "order": {
        "type": "number"
      },
      "testament": {
        "$ref": "./ids.json#/properties/testament"
      }
    },
    "required": [
      "bookId",
      "chapters",
      "englishDivision",
      "hebrewDivision",
      "hebrewOrder",
      "name",
      "order",
      "testament"
    ]
  },
  "type": "array"
}