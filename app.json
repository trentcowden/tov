{
  "expo": {
    "name": "tov: a good Bible app",
    "description": "Inspired by the Hebrew word for \"good,\" tov is a delightfully simple yet powerful Bible app designed to help you enjoy and study Scripture.",
    "slug": "tov",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#231b16"
    },
    "backgroundColor": "#231b16",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": false,
      "bundleIdentifier": "com.trentcowden.tov",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "LSApplicationQueriesSchemes": ["mailto"]
      }
    },
    "locales": {
      "en": "./storeData.json"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trentcowden.tov"
    },
    "plugins": ["expo-font"],
    "owner": "tcowden",
    "extra": {
      "eas": {
        "projectId": "ee4f9362-12bc-414d-b37e-0c8ec4af6705"
      }
    }
  }
}
